{
  "name": "@claw-network/core",
  "version": "0.2.0",
  "description": "Core cryptographic primitives and utilities for the ClawNet protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/claw-network/clawnet.git",
    "directory": "packages/core"
  },
  "keywords": [
    "clawnet",
    "agent",
    "decentralized",
    "crypto",
    "p2p"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "default": "./dist/crypto/index.js"
    },
    "./encoding": {
      "types": "./dist/encoding/index.d.ts",
      "default": "./dist/encoding/index.js"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "default": "./dist/identity/index.js"
    },
    "./p2p": {
      "types": "./dist/p2p/index.d.ts",
      "default": "./dist/p2p/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "default": "./dist/protocol/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "default": "./dist/storage/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "test": "vitest run",
    "lint": "eslint src --max-warnings=0"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.2",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.1",
    "@libp2p/autonat": "^3.0.10",
    "@libp2p/bootstrap": "^12.0.11",
    "@libp2p/dcutr": "^3.0.10",
    "@libp2p/identify": "^4.0.10",
    "@libp2p/kad-dht": "^16.1.3",
    "@libp2p/mdns": "^12.0.11",
    "@libp2p/ping": "^3.0.10",
    "@libp2p/tcp": "^11.0.10",
    "@multiformats/multiaddr": "^13.0.1",
    "@noble/curves": "^1.7.3",
    "@noble/ed25519": "^2.1.0",
    "@noble/hashes": "^1.6.3",
    "@scure/base": "^1.1.9",
    "@scure/bip39": "^1.2.2",
    "canonicalize": "^2.0.0",
    "level": "^9.0.0",
    "libp2p": "^3.1.3",
    "yaml": "^2.5.0"
  }
}
